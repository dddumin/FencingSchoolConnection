<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.dumin.ApprenticeInfoController">

    <Label layoutX="14.0" layoutY="56.0" text="Фамилия" />
    <Label layoutX="14.0" layoutY="95.0" text="Имя" />
    <Label layoutX="14.0" layoutY="137.0" text="Отчество" />
    <TextField fx:id="textFieldSurName" editable="false" layoutX="122.0" layoutY="52.0" />
    <TextField fx:id="textFieldName" editable="false" layoutX="122.0" layoutY="91.0" />
    <TextField fx:id="textFieldPatronymic" editable="false" layoutX="122.0" layoutY="133.0" />
    <Label fx:id="labelPhoneNumber" layoutX="14.0" layoutY="179.0" text="Номер телефона" />
    <TextField fx:id="textFieldPhoneNumber" layoutX="122.0" layoutY="175.0" />
    <Button layoutX="122.0" layoutY="213.0" mnemonicParsing="false" onAction="#update" prefHeight="25.0" prefWidth="149.0" text="Обновить ученика" />
