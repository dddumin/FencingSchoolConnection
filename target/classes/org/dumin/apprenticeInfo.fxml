   <ListView fx:id="listViewTraining" layoutX="300.0" layoutY="52.0" prefHeight="148.0" prefWidth="287.0" />
   <Label layoutX="300.0" layoutY="28.0" text="Список тренировок" />
   <Button layoutX="300.0" layoutY="224.0" mnemonicParsing="false" onAction="#addTraining" prefHeight="25.0" prefWidth="143.0" text="Добавить тренировку" />
   <Button layoutX="300.0" layoutY="262.0" mnemonicParsing="false" onAction="#deleteTraining" prefHeight="25.0" prefWidth="143.0" text="Удалить тренировку" />
</AnchorPane>
